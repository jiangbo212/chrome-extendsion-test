{
    "manifest_version": 3,
    "name": "Screenshot Extension",
    "version": "1.0",
    "icons": {
      "128": "128.png"
    },
    "permissions": ["activeTab"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "128": "128.png"
      }
    }
  }
  